{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 3001",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint src tests",
    "lint:prettier": "prettier --check src tests",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix src tests",
    "format:prettier": "prettier --write src tests",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-server-and-test start http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open",
    "test:e2e:update-snapshot": "start-server-and-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "storycap": "storycap --serverTimeout 60000 --serverCmd 'start-storybook --ci -p 9002' http://localhost:9002"
  },
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "formik": "2.2.9",
    "formik-material-ui": "3.0.1",
    "graphql": "15.5.1",
    "isomorphic-unfetch": "3.1.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-table": "7.7.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@apollo/link-schema": "2.0.0-beta.3",
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "@graphql-codegen/typescript-react-apollo": "2.3.0",
    "@next/bundle-analyzer": "11.0.1",
    "@storybook/addons": "6.3.2",
    "@storybook/react": "6.3.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@twihike/eslint-config": "0.1.15",
    "@twihike/prettier-config": "0.1.15",
    "@types/cypress-image-snapshot": "3.1.5",
    "@types/graphql-iso-date": "3.4.0",
    "@types/jest": "26.0.23",
    "@types/node": "16.0.0",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.8",
    "@types/react-table": "7.7.1",
    "@types/yup": "0.29.12",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "cypress": "7.6.0",
    "cypress-image-snapshot": "4.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "34.0.1",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "5.0.0",
    "jest": "27.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "8.3.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.2",
    "react-test-renderer": "17.0.2",
    "reg-cli": "0.17.0",
    "start-server-and-test": "1.12.5",
    "storycap": "3.0.4",
    "tailwindcss": "1.9.6",
    "tailwindcss-theming": "2.4.3",
    "typescript": "4.3.5"
  }
}
