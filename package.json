{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 3001",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "format": "eslint --fix --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-server-and-test start http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open",
    "test:e2e:update-snapshot": "start-server-and-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "storycap": "storycap --serverTimeout 60000 --serverCmd 'start-storybook --ci -p 9002' http://localhost:9002"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.41",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@material-ui/core": "4.9.8",
    "@material-ui/icons": "4.9.1",
    "formik": "2.1.4",
    "formik-material-ui": "2.0.0-beta.1",
    "graphql": "14.6.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-table": "7.0.4",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@apollo/link-schema": "2.0.0-beta.3",
    "@babel/core": "7.8.7",
    "@cypress/webpack-preprocessor": "4.1.3",
    "@fullhuman/postcss-purgecss": "2.1.0",
    "@graphql-codegen/cli": "1.13.1",
    "@graphql-codegen/introspection": "1.13.1",
    "@graphql-codegen/typescript": "1.13.1",
    "@graphql-codegen/typescript-operations": "1.13.1",
    "@graphql-codegen/typescript-react-apollo": "1.13.1",
    "@next/bundle-analyzer": "9.3.4",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.2",
    "@testing-library/user-event": "10.0.1",
    "@types/cypress-image-snapshot": "3.1.2",
    "@types/graphql-iso-date": "3.3.3",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-table": "7.0.13",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-jest": "25.2.6",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "cypress": "4.3.0",
    "cypress-image-snapshot": "3.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "4.0.7",
    "jest": "25.2.7",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.2",
    "react-test-renderer": "16.13.1",
    "reg-cli": "0.16.0",
    "start-server-and-test": "1.10.11",
    "storycap": "2.3.5",
    "tailwindcss": "1.2.0",
    "tailwindcss-theming": "2.4.3",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3"
  }
}
