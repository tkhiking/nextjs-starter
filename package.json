{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 3001",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "format": "eslint --fix --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-server-and-test start http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open",
    "test:e2e:update-snapshot": "start-server-and-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "storycap": "storycap --serverTimeout 60000 --serverCmd 'start-storybook --ci -p 9002' http://localhost:9002"
  },
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "formik": "2.2.6",
    "formik-material-ui": "3.0.1",
    "graphql": "15.4.0",
    "isomorphic-unfetch": "3.1.0",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-table": "7.6.2",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@apollo/link-schema": "2.0.0-beta.3",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@next/bundle-analyzer": "10.0.3",
    "@storybook/addons": "6.1.11",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@twihike/eslint-config": "0.1.12",
    "@twihike/prettier-config": "0.1.12",
    "@types/cypress-image-snapshot": "3.1.2",
    "@types/graphql-iso-date": "3.4.0",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-table": "7.0.26",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "cypress": "6.1.0",
    "cypress-image-snapshot": "4.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.16.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "24.0.0",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "5.0.0",
    "jest": "26.6.3",
    "postcss": "8.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "reg-cli": "0.16.4",
    "start-server-and-test": "1.11.6",
    "storycap": "3.0.3",
    "tailwindcss": "1.9.6",
    "tailwindcss-theming": "2.4.3",
    "typescript": "4.1.3"
  }
}
