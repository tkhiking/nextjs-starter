{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 3001",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "format": "eslint --fix --ext .js,.ts,.tsx --ignore-pattern generated src tests",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http://localhost:3001 test:e2e:inner",
    "test:e2e:inner": "cypress run",
    "test:e2e:open": "start-server-and-test start http://localhost:3001 test:e2e:open:inner",
    "test:e2e:open:inner": "cypress open",
    "test:e2e:update-snapshot": "start-server-and-test start http://localhost:3001 test:e2e:update-snapshot:inner",
    "test:e2e:update-snapshot:inner": "cypress run --env updateSnapshots=true",
    "graphql-typedefs": "node scripts/graphql-typedefs",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook --ci -p 9001",
    "storycap": "storycap --serverTimeout 60000 --serverCmd 'start-storybook --ci -p 9002' http://localhost:9002"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.50",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "formik": "2.1.4",
    "formik-material-ui": "2.0.0-beta.1",
    "graphql": "15.0.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-table": "7.1.0",
    "yup": "0.29.0"
  },
  "devDependencies": {
    "@apollo/link-schema": "2.0.0-beta.3",
    "@graphql-codegen/cli": "1.14.0",
    "@graphql-codegen/introspection": "1.14.0",
    "@graphql-codegen/typescript": "1.14.0",
    "@graphql-codegen/typescript-operations": "1.14.0",
    "@graphql-codegen/typescript-react-apollo": "1.14.0",
    "@next/bundle-analyzer": "9.4.2",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.3.5",
    "@twihike/node-toolchain": "^0.1.5",
    "@types/cypress-image-snapshot": "3.1.2",
    "@types/graphql-iso-date": "3.3.3",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-table": "7.0.18",
    "@types/yup": "0.29.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "cypress": "4.6.0",
    "cypress-image-snapshot": "3.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-plugin-cypress": "2.10.3",
    "graphql-iso-date": "3.6.1",
    "graphql-tools": "5.0.0",
    "jest": "26.0.1",
    "postcss-preset-env": "6.7.0",
    "react-test-renderer": "16.13.1",
    "reg-cli": "0.16.0",
    "start-server-and-test": "1.11.0",
    "storycap": "2.3.5",
    "tailwindcss": "1.4.6",
    "tailwindcss-theming": "2.4.3",
    "typescript": "3.9.3"
  }
}
